ogit.ITIL:incidentLevel
	a owl:DatatypeProperty;
	rdfs:subPropertyOf ogit:Attribute;
	rdfs:label "incidentLevel";
	ogit:original <http://wwwvs.cs.hs-rm.de/itil#incidentLevel>;
	dcterms:description "The integer value that represents the level of importance of a specific itil:Incident. In our pilot project, the itil:IncidentLevel is calculated from the itil:incidentUrgency and the itil:incidentGroupType that reported the itil:Incident (itil:hasIncidentGroup property). The level codes range from 0 to 5 (5 is the highest level of importance).";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
	dcterms:valid "start=2016-09-20;";
	dcterms:creator "OGIT Importer";
	dcterms:created "2016-09-20";
	dcterms:modified "2016-09-20";
	ogit:admin-contact "arago GmbH";
	ogit:tech-contact "arago GmbH";
	ogit:hide "false";
	ogit:validation-type "";
	ogit:validation-parameter "";
	ogit:history (
		[
			dcterms:identifier "1";
			dcterms:date "2016-09-20";
			dcterms:description "initial";
			dcterms:creator "OGIT Importer";
		]
	);
.
