@prefix dcterms:                <http://purl.org/dc/terms/> .
@prefix ogit.Auth:              <http://www.purl.org/ogit/Auth/> .

ogit.Auth:authenticates
	a owl:ObjectProperty;
	rdfs:subPropertyOf ogit:Verb;
	rdfs:label "authenticates";
	dcterms:description "Denoting authentication of something.";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
	dcterms:valid "start=2018-01-01;";
	dcterms:creator "arago GmbH";
	dcterms:created "2018-01-01";
	dcterms:modified "2018-01-01";
	ogit:admin-contact "arago GmbH";
	ogit:tech-contact "arago GmbH";
	ogit:history (
		
	);
	ogit:allowed (
		[
			ogit:from ogit.Auth:Account;
			ogit:to ogit.Auth:Person;
		]
	);
